                    "description": "Enable linking for route."
                },
                "Laravel.translation.diagnostics": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable diagnostics for translation."
                },
                "Laravel.translation.hover": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable hover information for translation."
                },
                "Laravel.translation.link": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable linking for translation."
                },
                "Laravel.view.diagnostics": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable diagnostics for view."
                },
                "Laravel.view.hover": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable hover information for view."
                },
                "Laravel.view.link": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable linking for view."
                }
            }
        },
        "commands": [
            {
                "command": "laravel.run-test",
                "title": "Laravel: Run Test at Cursor"
            },
            {
                "command": "laravel.run-test-file",
                "title": "Laravel: Run Tests in File"
            },
            {
                "command": "laravel.run-test-suite",
                "title": "Laravel: Run Test Suite"
            },
            {
                "command": "laravel.run-test-previous",
                "title": "Laravel: Run Previous Test"
            }
        ],
        "keybindings": [
            {
                "key": "cmd+k cmd+r",
                "command": "laravel.run",
                "when": "editorLangId == php"
            },
            {
                "key": "cmd+k cmd+f",
                "command": "laravel.run-file",
                "when": "editorLangId == php"
            },
            {
                "key": "cmd+k cmd+p",
                "command": "laravel.run-previous",
                "when": "editorLangId == php"
            }
        ]
    },
    "scripts": {
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "vscode-test",
        "compile": "npm run check-types && node esbuild.js",
        "check-types": "tsc --noEmit",
        "watch": "npm-run-all -p watch:*",
        "watch:esbuild": "node esbuild.js --watch",
        "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
        "vscode:prepublish": "npm run package",
        "package": "npm run check-types && node esbuild.js --production"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.6",
        "@types/node": "18.x",
        "@types/vscode": "^1.89.0",
        "@typescript-eslint/eslint-plugin": "^7.7.1",
        "@typescript-eslint/parser": "^7.7.1",
        "@vscode/test-cli": "^0.0.9",
        "@vscode/test-electron": "^2.3.9",
        "esbuild": "^0.21.5",
        "eslint": "^8.57.0",
        "typescript": "^5.4.5"
    },
    "dependencies": {
        "@microsoft/vscode-file-downloader-api": "^1.0.1",
        "find-up": "^7.0.0",
        "glob": "^11.0.0",
        "npm-run-all": "^4.1.5",
        "php-parser": "^3.1.5",
        "vscode-html-languageservice": "^5.2.0",
        "vscode-languageclient": "^9.0.1",
        "vscode-languageserver-textdocument": "^1.0.11",
        "vscode-languageserver-types": "^3.17.5"
    },
    "extensionDependencies": [
        "mindaro-dev.file-downloader"
    ]
}
