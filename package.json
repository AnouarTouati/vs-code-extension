{
    "name": "laravel",
    "publisher": "Laravel",
    "displayName": "Laravel",
    "description": "Official VS Code extension for Laravel",
    "version": "0.1.3",
    "engines": {
        "vscode": "^1.89.0"
    },
    "icon": "laravel-vs-code-icon.png",
    "homepage": "https://github.com/laravel/vs-code-extension",
    "repository": {
        "type": "git",
        "url": "https://github.com/laravel/vs-code-extension"
    },
    "bugs": {
        "url": "https://github.com/laravel/vs-code-extension/issues"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:php"
    ],
    "main": "./dist/extension.js",
    "grammars": [
        {
            "language": "blade",
            "scopeName": "text.html.php.blade",
            "path": "./syntaxes/blade.tmLanguage.json",
            "embeddedLanguages": {
                "source.php": "php",
                "source.css": "css",
                "source.js": "javascript"
            }
        }
    ],
    "contributes": {
        "languages": [
            {
                "id": "blade",
                "aliases": [
                    "Blade",
                    "blade"
                ],
                "extensions": [
                    ".blade.php"
                ],
                "configuration": "./blade.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "blade",
                "scopeName": "text.html.php.blade",
                "path": "./syntaxes/blade.tmLanguage.json",
                "embeddedLanguages": {
                    "source.php": "php",
                    "source.css": "css",
                    "source.js": "javascript"
                }
            }
        ],
        "configuration": {
            "title": "Laravel",
            "properties": {
                "Laravel.phpCommand": {
                    "type": "string",
                    "description": "Template for running PHP code. Use {code} as a placeholder for the code to run. e.g. `php -r \"{code}\"`"
                },
                "Laravel.appBinding.diagnostics": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable diagnostics for appBinding."
                },
                "Laravel.appBinding.hover": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable hover information for appBinding."
                },
                "Laravel.appBinding.link": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable linking for appBinding."
                },
                "Laravel.asset.diagnostics": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable diagnostics for asset."
                },
                "Laravel.asset.hover": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable hover information for asset."
                },
                "Laravel.asset.link": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable linking for asset."
                },
                "Laravel.auth.diagnostics": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable diagnostics for auth."
                },
                "Laravel.auth.hover": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable hover information for auth."
                },
                "Laravel.auth.link": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable linking for auth."
                },
                "Laravel.config.diagnostics": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable diagnostics for config."
                },
                "Laravel.config.hover": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable hover information for config."
                },
                "Laravel.config.link": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable linking for config."
                },
                "Laravel.controllerAction.diagnostics": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable diagnostics for controllerAction."
                },
                "Laravel.controllerAction.hover": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable hover information for controllerAction."
                },
                "Laravel.controllerAction.link": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable linking for controllerAction."
                },
                "Laravel.env.diagnostics": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable diagnostics for env."
                },
                "Laravel.env.hover": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable hover information for env."
                },
                "Laravel.env.link": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable linking for env."
                },
                "Laravel.inertia.diagnostics": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable diagnostics for inertia."
                },
                "Laravel.inertia.hover": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable hover information for inertia."
                },
                "Laravel.inertia.link": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable linking for inertia."
                },
                "Laravel.middleware.diagnostics": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable diagnostics for middleware."
                },
                "Laravel.middleware.hover": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable hover information for middleware."
                },
                "Laravel.middleware.link": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable linking for middleware."
                },
                "Laravel.mix.diagnostics": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable diagnostics for mix."
                },
                "Laravel.mix.hover": {
                    "type": "boolean",
                    "default": true,
                    "generated": true,
                    "description": "Enable hover information for mix."
                },
                "Laravel.mix.link": {
                    "type": "boolean",
                    "default": true,
